generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Product {
  id          Int      @id @default(autoincrement())
  name        String
  description String?
  imageUrl    String? 
  price       Float
  inStock     Boolean   @default(true)
  category    String
  createdAt   DateTime @default(now())
}
